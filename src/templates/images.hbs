{{#each this}}
<li class="galley_item">
    <img class="galley_img" src="{{webformatURL}}" alt="" data-source="{{largeImageURL}}" data-idx="${index} += 1" />

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
        </p>
    </div>

</li>
{{/each}}